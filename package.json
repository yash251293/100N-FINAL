{
  "name": "culturefix-full-stack",
  "version": "1.0.0",
  "description": "CultureFix - Full Stack Application",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cd \"Final UI\" && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd \"Final UI\" && npm install && npm run build",
    "build:backend": "cd backend && npm install",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:frontend": "cd \"Final UI\" && npm start",
    "start:backend": "cd backend && npm start",
    "deploy:setup": "chmod +x deploy.sh && ./deploy.sh"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": ["culture", "feedback", "management", "full-stack"],
  "author": "Yash",
  "license": "MIT"
} 